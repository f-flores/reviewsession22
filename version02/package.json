    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "concurrently": "^4.1.1",
    "eslint": "5.3.0",
    "eslint-config-airbnb": "17.1.1",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-import": "2.18.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.14.2",
    "exports-loader": "^0.7.0",
    "html-webpack-plugin": "^3.2.0",
